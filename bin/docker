#!/usr/bin/env bash

export VAGRANT_DEFAULT_PROVIDER="docker"

function main {
  local shome="$(cd -P -- "${BASH_SOURCE%/*}/.." && pwd -P)"

  source "$shome/script/profile"

  if [[ -x "$(which "parity-${1:-}" 2>&- || true)" ]]; then
    exec parity "$@"
  else
    exec "$(which /usr/local/bin/docker /usr/bin/docker)" "$@"
  fi
}

source sub "$0" "$@"
